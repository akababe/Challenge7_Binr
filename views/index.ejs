<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Open+Sans" />
    <link href="asset/css/bootstrap.min.css" rel="stylesheet">
    <link href="asset/css/style.css" rel="stylesheet">


    <title><%= judul %> </title>
    <style>

    </style>
</head>

<body>
    <!-- navbar start -->
    <nav class="navbar navbar-expand-lg navbar-dark">
        <div class=" container">
            <a class="navbar-brand" href="#">LOGO</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ms-auto me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="#">HOME</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="#">WORK</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="#">CONTACT</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="#">ABOUT ME</a>
                    </li>
                </ul>
                <div class="d-flex">
                    <% 
                        if (email == undefined) {
                            beforeLogin= "";
                            afterLogin="hidden";
                        }else{
                            beforeLogin ="hidden"
                            afterLogin=""
                        }    
                    %>
                    <a class="nav-link" <%= afterLogin %>><%= email %></a>
                    <a class="nav-link " aria-current="page" href="/logout" <%= afterLogin %>>LOGOUT</a>
                    <a class="nav-link " aria-current="page" href="/register" <%= beforeLogin %>>SIGN UP</a>
                    <a class="nav-link " aria-current="page" href="/login" <%= beforeLogin %>>LOGIN</a>
                    <!-- <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success" type="submit">Search</button> -->
                </div>
            </div>
        </div>
    </nav>
    <!-- navbar end -->

    <!-- main start -->
    <section class="jumbotron d-flex align-items-center" id="main">
        <div class="container-fluid ">
            <div class="main">
                <p class="text-main">PLAY TRADITIONAL GAME</p>
                <p class=" text-1">Experience new traditional game play</p>
                <a class="main btn btn-warning text-btn" href="/game">TRIAL</a>

            </div>
        </div>
        <div class="container-fluid position-absolute text-center" style="bottom: 30px; width: 100%;">
            <div class="row d-flex justify-content-center">
                <div class="col-2">
                    <p class=" text-footer">THE STORY</p>
                    <a href="#gallery" style="text-decoration: none;">
                        <img src="asset/img/scroll down.svg" alt="">
                    </a>
                </div>
            </div>
        </div>

    </section>
    <!-- main end -->

    <!-- gallery start -->
    <section class="gallery d-flex align-items-center" id="gallery">
        <div class="container">
            <div class="row ">
                <div class="content-galeri col-lg-4 ">
                    <p class="text-1">What's so special?</p>
                    <p class="text-2" style="margin-top: 2px;">THE GAMES</p>
                </div>

                <div class="content-galeri col-lg-8">
                    <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
                        <div class="carousel-indicators">
                            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0"
                                class="active" aria-current="true" aria-label="Slide 1"></button>
                            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1"
                                aria-label="Slide 2"></button>
                            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2"
                                aria-label="Slide 3"></button>
                        </div>
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="asset/img/rockpaperstrategy-1600.jpg" class="d-block w-100" alt="...">
                            </div>
                            <div class="carousel-item">
                                <img src="asset/img/rockpaperstrategy-1600.jpg" class="d-block w-100" alt="...">
                            </div>
                            <div class="carousel-item">
                                <img src="asset/img/rockpaperstrategy-1600.jpg" class="d-block w-100" alt="...">
                            </div>
                        </div>
                        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators"
                            data-bs-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="visually-hidden">Previous</span>
                        </button>
                        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators"
                            data-bs-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="visually-hidden">Next</span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- gallery end -->

    <!-- feature start -->
    <section class="feature d-flex align-items-center" id="feature">

        <div class="container">
            <div class="row d-flex justify-content-center justify-content-md-end ">
                <div class="col-7 col-sm-7 col-md-5">
                    <p class="text-1 ">What's so special?</p>
                    <p class="text-2 ">FEATURE</p>
                    <ul class="feature-list">
                        <li class="active text-3" onclick="myFunction()">TRADITIONAL GAMES</li>

                        <p class="text-4" id="child-list">If you miss your childhood, we provide many traditional games
                            here</p>
                        <li class="text-3">GAME SUIT</li>
                        <li class="text-3">TBD</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <!-- feature end -->

    <!-- requirements start -->
    <section class="requirements  d-flex align-items-center justify-content-center justify-content-sm-start"
        id="requirements">
        <div class="container">
            <div class="row ">
                <div class="col-xl-4">
                    <p class="text-2">SYSTEM REQUIREMENTS</p>
                    <table>
                        <tr>
                            <td>
                                <p class="text-3">OS:</p>
                                <p class="text-4">Windows 7 64-bit only (No OSX support at this time)</p>
                            </td>
                            <td>
                                <p class="text-3">PROCESSOR:</p>
                                <p class="text-4">Intel Core 2 Duo @ 2.4 GHZ or AMD Athlon X2 @ 2.8 GHZ</p>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <p class="text-3">MEMORY:</p>
                                <p class="text-4">4 GB RAM</p>
                            </td>
                            <td>
                                <p class="text-3">STgiORAGE:</p>
                                <p class="text-4">8 GB available space</p>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                <p class="text-3">GRAPHICS:</p>
                                <p class="text-4">NVIDIA GeForce GTX 660 2GB or AMD Radeon HD 7850 2GB DirectX11 (Shader
                                    Model 5)</p>
                            </td>

                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </section>
    <!-- requirements end -->

    <!-- scores start -->
    <section class="scores d-flex align-items-center" id="quotes">
        <div class="container">
            <div class="row d-flex align-items-center ">
                <div class="col-lg-6 text-center text-lg-start mb-5">
                    <p class="text-2">TOP SCORES</p>
                    <p class="text-4">This top score from various games provided on this platform</p>
                    <a class="btn top-scores btn-warning text-btn" href="">see more</a>
                </div>

                <div class="col-lg-6">
                    <div
                        class="row container-quotes p-2 d-flex justify-content-center justify-content-lg-end align-items-center">
                        <div class="col-12 col-sm-9 col-lg-9 quotes" style="height: 95%;">
                            <div class="row">
                                <div class="col-3">
                                    <img class="rounded-circle " src="asset/img/evan.jpg" alt="">
                                </div>
                                <div class="col-8">
                                    <p class="text-3">Evant Lahti</p>
                                    <p class="text-4" style="opacity: 0.5; margin-top: 1.25rem;">PC Gamer</p>
                                </div>
                                <div class="col-1"><img src="asset/img/twitter.svg" alt=""></div>
                            </div>
                            <div class="row pt-3">
                                <p class="text-4">“One of my gaming highlights of the year.”</p>
                                <p class="text-5">June 18, 2021</p>
                            </div>
                        </div>
                    </div>
                    <div
                        class="row container-quotes p-2 d-flex justify-content-lg-start justify-content-center align-items-center">
                        <div class="col-12 col-sm-9 col-lg-9 quotes" style="height: 95%;">
                            <div class="row">
                                <div class="col-3">
                                    <img class="rounded-circle " src="asset/img/evan.jpg" alt="">
                                </div>
                                <div class="col-8">
                                    <p class="text-3">Evant Lahti</p>
                                    <p class="text-4" style="opacity: 0.5; margin-top: 1.25rem;">PC Gamer</p>
                                </div>
                                <div class="col-1"><img src="asset/img/twitter.svg" alt=""></div>
                            </div>
                            <div class="row pt-3">
                                <p class="text-4">“One of my gaming highlights of the year.”</p>
                                <p class="text-5">June 18, 2021</p>
                            </div>
                        </div>
                    </div>
                    <div
                        class="row container-quotes  p-2 d-flex justify-content-lg-end justify-content-center align-items-center">
                        <div class="col-12 col-sm-9 col-lg-9 quotes" style=" height: 95%;">
                            <div class="row">
                                <div class="col-3">
                                    <img class="rounded-circle " src="asset/img/evan.jpg" alt="">

                                </div>
                                <div class="col-8">
                                    <p class="text-3">Evant Lahti</p>
                                    <p class="text-4" style="opacity: 0.5; margin-top: 1.25rem;">PC Gamer</p>
                                </div>
                                <div class="col-1"><img src="asset/img/twitter.svg" alt=""></div>
                            </div>
                            <div class="row pt-3">
                                <p class="text-4">“One of my gaming highlights of the year.”</p>
                                <p class="text-5">June 18, 2021</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>
    <!-- scores end -->

    <section class="text-white footer d-flex justify-content-center">
        <div class="container " style="margin-bottom: auto; margin-top: auto">
            <div class="row d-flex  justify-content-center align-items-center">
                <div class="col-lg-4 d-none d-lg-block gradient">
                    <div class="skull">
                    </div>
                    <!-- <img class="img-footer" src="asset/img/skull.png" alt=""> -->
                </div>
                <div class="col-11 col-lg-6">
                    <p class="text-1">Want to stay in touch?</p>
                    <p class="text-6">newsletter SUBSCRIBE </p>
                    <p class="text-4 ">In order to start receiving our news, all you have to do is enter your email
                        address. Everything
                        else will be taken care of by us. We will send you emails containing information about game. We
                        don’t spam.</p>

                    <form class="row g-3 mt-5">

                        <div class="col-7 input-email">
                            <label class="text-5" for="inputPassword2" class="visually">your email address</label>
                            <input type="email" class="form-control form-footer" id="inputPassword2">
                        </div>
                        <div class="col-5">
                            <button type="submit" class="btn btn-warning mb-3 btn-email text-btn">Subscribe now</button>
                        </div>
                    </form>
                </div>
            </div>


        </div>
    </section>
